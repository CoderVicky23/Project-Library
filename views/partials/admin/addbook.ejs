<div class="paneContent tab-pane fade p-5" id="nav-profile" role="tabpanel" aria-labelledby="nav-contact-tab">
    <form class="addBookForm p-5">
        <div class="form-group">
            <label for="exampleInputBookName">Book Name</label>
            <input type="text" class="form-control" id="exampleInputBookName" placeholder="Name of the Book"><br>
        </div>
        <div class="form-group">
            <label for="exampleBookAuthor">Author</label>
            <input type="text" class="form-control" id="exampleBookAuthor" placeholder="Author Name"><br>
        </div>
        <div class="input-group mb-3 form-group">
            <div class="input-group-prepend">
              <label class="input-group-text" for="exampleBookCategory">Category</label>
            </div>
            <select class="custom-select" id="exampleBookCategory" placeholder="Choose...">
              <option value="maths">Maths</option>
              <option value="cs">Computer Science</option>
              <option value="english">English</option>
              <option value="geography">Geography</option>
              <option value="hindi">Hindi</option>
              <option value="psychology">Psychology</option>
              <option value="chemistry">Chemistry</option>
              <option value="biology">Biology</option>
              <option value="physics">Physics</option>
            </select>
          </div>
        <div class="form-group">
            <label for="exampleBookPrice">Price</label>
            <input type="text" class="form-control" id="exampleBookPrice" placeholder="Book Price"><br>
        </div>
        <div class="form-group">
            <label for="exampleBookQuantity">Quantity of Books</label>
            <input type="text" class="form-control" id="exampleBookQuantity" placeholder="Book Quantity"><br>
        </div>
        <div class="form-group">
            <button class="btn btn-outline-success my-2 my-sm-0" type="button" id="bookAdd">Enter New Book</button>
        </div>
    </form>
    
    <div class="addBookFieldEmptyPop alert alert-dark alert-dismissible hide">
        <strong>Field Can Not Be Empty!</strong>
    </div>
    <div id="userInfo"></div>
</div>
<script src="/static/adminJS/addbook.js"></script>